package mainpackage;

import java.io.File;
import java.io.FileNotFoundException;
import java.util.Scanner;

public class SpeckCipher {
	public String word;
	
	public SpeckCipher()
	{
		Scanner keyboard;
		try {
			keyboard = new Scanner(new File("Desktop\\DaneMoebiusProject\\DaneEclipseProject\\src\\mainpackage\\fileRead.txt"));
		} catch (FileNotFoundException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		word=keyboard.next();
	}
	public byte encrypt(String input)
	{
		return 44;
	}
	public String decrypt(byte input)
	{
		return "";
	}
	public String toString()
	{
		return word;
	}
}
