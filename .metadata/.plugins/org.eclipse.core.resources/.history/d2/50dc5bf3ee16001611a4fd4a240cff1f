package mainpackage;

import java.util.Scanner;

public class SpeckCipher {
	public String word;
	
	public SpeckCipher()
	{
		Scanner keyboard= new Scanner("Desktop\\DaneMoebiusProject\\DaneEclipseProject\\src\\mainpackage\\fileRead.txt");
		word=keyboard.next();
	}
	public byte encrypt(String input)
	{
		return 44;
	}
	public String decrypt(byte input)
	{
		return "";
	}
	public String toString()
	{
		return word;
	}
}
